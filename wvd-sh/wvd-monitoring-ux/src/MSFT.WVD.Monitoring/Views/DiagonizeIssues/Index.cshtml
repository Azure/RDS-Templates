                                                                                                                                                                                                                                { "ClientOS", @d.ClientOS },
                                                                                                                                                                                                                                { "ClientIPAddress", @d.ClientIPAddress },
                                                                                                                                                                                                                                { "startTime", @d.startTime.ToString() },
                                                                                                                                                                                                                                { "endTime", @d.endTime.ToString() },
                                                                                                                                                                                                                                {"SessionHostName",d.SessionHostName },
                                                                                                                                                                                                                                {"SessionHostPoolName",d.SessionHostPoolName }
