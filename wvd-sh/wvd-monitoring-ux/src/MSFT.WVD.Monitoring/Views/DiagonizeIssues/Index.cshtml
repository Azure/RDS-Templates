                            <th asp-for="Type">Type</th>
                            <th asp-for="Outcome">Outcome</th>
                            <th asp-for="Error Type">Error Type</th>
                            <th asp-for="Error">Error</th>
                            <th asp-for="Tenant">Tenant</th>
